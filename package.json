{
  "name": "GoalTracker",
  "version": "1.0.0",
  "description": "A tool for ...",
  "main": "main.js",
  "scripts": {
    "watch:sass": "node-sass sass/main.scss css/style.css -w",
    "prefix:css": "postcss --use autoprefixer -b 'last 10 versions' css/style.comp.css -o css/style.prefix.css",
    "build:css": "npm-run-all watch:sass prefix:css",
    "build:webpack": "webpack --mode development --watch",
    "start": "npm-run-all --parallel build:css build:webpack"  
  },
  "author": "Galina",
  "license": "MIT",
  "devDependencies": {
    "lodash": "^3.10.1",
    "autoprefixer": "^7.2.6",
    "concat": "^1.0.3",
    "node-sass": "^4.7.2",
    "npm-run-all": "^4.1.2",
    "postcss-cli": "^4.1.1"
  },
  "dependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "webpack": "^4.3.0",
    "webpack-cli": "^2.0.13",
    "webpack-loader": "0.0.1"
  }
}
